<div class="container">
  <div class="col-lg-12 ">
      <div class="panel panel-default">
        <div class="panel panel-title col-md-offset-4">
          <h1>All Shorten links</h1>
        </div>
        <div class="panel-body">
          <%= render 'api_shortners/form' %>
        </div>
    </div>
  </div>
</div>

<% @shortners.each do |shortner| %>
  <div class="col-lg-3">
      <div class="panel panel-default">
        <div class="panel-body">
            <p class="short_url"> <%= link_to shortner.mask_url, api_shortner_path(shortner.slug), target: '_blank' %></p>
          </div
        </div>
    </div>
  </div>
<% end %>
<div class="shortner">
</div>
